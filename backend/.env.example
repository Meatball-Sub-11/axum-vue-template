# Environment variables for the application.
# Copy this file to .env and replace the values for your local setup.

# Used for signing JWTs. Generate a long, random string.
JWT_SECRET=""

# Connection string for your local PostgreSQL database.
DATABASE_URL="postgres://postgres:password@localhost:5432/template_db"

# Port for the Axum backend server.
APP_PORT=3000

# Origin URL for the frontend development server (for CORS).
FRONTEND_ORIGIN="http://localhost:5173"

# Default log level. Options: trace, debug, info, warn, error
RUST_LOG="info"
